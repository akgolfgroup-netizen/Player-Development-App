{
  "name": "iup-golf-backend",
  "version": "1.0.1",
  "description": "IUP Golf Academy Backend - Fastify + Prisma + TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prebuild": "prisma generate",
    "build": "swc src -d dist --strip-leading-paths",
    "postbuild": "[ -d src/templates ] && cp -r src/templates dist/ || true; npx swc scripts/wait-for-db.ts -o dist/wait-for-db.js",
    "postinstall": "prisma generate || true",
    "build:tsc": "tsc",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "start:prod": "node dist/wait-for-db.js && prisma migrate deploy && node dist/server.js",
    "db:wait": "tsx scripts/wait-for-db.ts",
    "db:migrate:prod": "tsx scripts/wait-for-db.ts && prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:reset": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test prisma db push --force-reset --skip-generate",
    "test:clean": "npm run test:reset && npm run prisma:seed:test && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "seed:demo": "tsx scripts/seed-demo.ts",
    "prisma:seed:test": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test tsx prisma/seed.ts",
    "prisma:push:test": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test prisma db push --skip-generate",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs:export-openapi": "tsx scripts/export-openapi.ts",
    "docs:export-postman": "npm run docs:export-openapi && tsx scripts/openapi-to-postman.ts",
    "docs:export-all": "npm run docs:export-postman"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "fastify",
    "prisma",
    "typescript",
    "golf",
    "training",
    "iup"
  ],
  "author": "Anders Kristiansen",
  "license": "UNLICENSED",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.957.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "8",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/websocket": "^10.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^10.32.1",
    "@sentry/profiling-node": "^10.32.1",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "adm-zip": "^0.5.16",
    "argon2": "^0.40.3",
    "axios": "^1.13.2",
    "bullmq": "^5.13.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^5.1.0",
    "googleapis": "^170.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.17.0",
    "speakeasy": "^2.0.0",
    "stripe": "^20.1.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.5",
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.16.0",
    "@types/sanitize-html": "^2.16.0",
    "@types/stripe": "^8.0.416",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "29",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^7.2.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
