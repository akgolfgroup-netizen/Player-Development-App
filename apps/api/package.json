{
  "name": "iup-golf-backend",
  "version": "1.0.0",
  "description": "IUP Golf Academy Backend - Fastify + Prisma + TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "swc src -d dist --strip-leading-paths",
    "build:tsc": "tsc",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:reset": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test prisma db push --force-reset --skip-generate",
    "test:clean": "npm run test:reset && npm run prisma:seed:test && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:seed:test": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test tsx prisma/seed.ts",
    "prisma:push:test": "DATABASE_URL=postgresql://iup_golf:dev_password@localhost:5432/iup_golf_test prisma db push --skip-generate",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "fastify",
    "prisma",
    "typescript",
    "golf",
    "training",
    "iup"
  ],
  "author": "Anders Kristiansen",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.957.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/websocket": "^10.0.1",
    "@prisma/client": "^5.22.0",
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "argon2": "^0.40.3",
    "axios": "^1.13.2",
    "bullmq": "^5.13.2",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.17.2",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
