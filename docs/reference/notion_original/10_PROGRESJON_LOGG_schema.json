{
  "database_name": "PROGRESJON_LOGG",
  "description": "Daily training log - Track session completion, quality, and player progress",
  "properties": {
    "Logg_ID": {
      "type": "title",
      "description": "Unique identifier (auto-generated: 'Player - Date')"
    },
    "Dato": {
      "type": "date",
      "description": "Training session date"
    },
    "Spiller": {
      "type": "relation",
      "related_database": "SPILLERE",
      "description": "Link to player profile"
    },
    "Økt_planlagt": {
      "type": "relation",
      "related_database": "TRENINGSØKTER",
      "description": "Planned session from schedule"
    },
    "Økt_type": {
      "type": "select",
      "options": [
        "Teknikk - Full Swing",
        "Teknikk - Short Game",
        "Putting",
        "Fysisk",
        "Mental",
        "Spill - Range",
        "Spill - Bane",
        "Turnering",
        "Annet"
      ],
      "description": "Session type (auto-filled from planned session or manual)"
    },
    "Gjennomført": {
      "type": "checkbox",
      "description": "Session completed (yes/no)"
    },
    "Grunn_ikke_gjennomført": {
      "type": "select",
      "options": [
        "Syk",
        "Skadet",
        "Vær",
        "Skole/eksamen",
        "Annet arrangement",
        "Utbrenthet/tretthet",
        "Utstyr/facility ikke tilgjengelig",
        "Annet"
      ],
      "description": "Reason if not completed"
    },
    "Varighet_planlagt": {
      "type": "number",
      "description": "Planned duration (minutes)"
    },
    "Varighet_faktisk": {
      "type": "number",
      "description": "Actual duration (minutes)"
    },
    "Varighet_differanse": {
      "type": "formula",
      "formula": "prop('Varighet_faktisk') - prop('Varighet_planlagt')",
      "description": "Difference from planned time"
    },
    "Intensitet_planlagt": {
      "type": "select",
      "options": ["Lav", "Moderat", "Høy", "Maks"],
      "description": "Planned intensity"
    },
    "Intensitet_faktisk": {
      "type": "select",
      "options": ["Lav", "Moderat", "Høy", "Maks"],
      "description": "Actual perceived intensity"
    },
    "Kvalitet": {
      "type": "select",
      "options": ["5 - Utmerket", "4 - Bra", "3 - Ok", "2 - Under forventning", "1 - Dårlig"],
      "description": "Session quality rating"
    },
    "Fokus": {
      "type": "select",
      "options": ["5 - Full fokus", "4 - God fokus", "3 - Moderat fokus", "2 - Distrahert", "1 - Manglet fokus"],
      "description": "Mental focus during session"
    },
    "Energinivå_før": {
      "type": "select",
      "options": ["5 - Høy energi", "4 - God energi", "3 - Normal", "2 - Litt sliten", "1 - Svært sliten"],
      "description": "Energy level before session"
    },
    "Energinivå_etter": {
      "type": "select",
      "options": ["5 - Energisk", "4 - Bra", "3 - Normal", "2 - Sliten", "1 - Utmattet"],
      "description": "Energy level after session"
    },
    "Hva_fungerte": {
      "type": "text",
      "description": "What worked well this session"
    },
    "Utfordringer": {
      "type": "text",
      "description": "Challenges or difficulties encountered"
    },
    "Nøkkellæring": {
      "type": "text",
      "description": "Key insight or learning from session"
    },
    "Neste_fokus": {
      "type": "text",
      "description": "What to focus on in next session"
    },
    "Spiller_notater": {
      "type": "text",
      "description": "Player's own notes and reflections"
    },
    "Trener_tilbakemelding": {
      "type": "text",
      "description": "Coach feedback and observations"
    },
    "Video_link": {
      "type": "url",
      "description": "Link to session video (if recorded)"
    },
    "Måloppnåelse": {
      "type": "select",
      "options": ["Overgått", "Oppnådd", "Delvis oppnådd", "Ikke oppnådd", "Ikke relevant"],
      "description": "Achievement of session goals"
    },
    "Spesifikke_resultater": {
      "type": "text",
      "description": "Measurable results (e.g., 'Made 8/10 putts from 3m', 'Driver carry: 245m avg')"
    },
    "Bruddpunkt_arbeid": {
      "type": "relation",
      "related_database": "BRUDDPUNKTER",
      "description": "Breaking points addressed in this session"
    },
    "Øvelser_utført": {
      "type": "relation",
      "related_database": "ØVELSER",
      "description": "Specific drills completed"
    },
    "Lokasjon": {
      "type": "select",
      "options": ["Range", "Putting green", "Shortgame område", "Bane", "Gym", "Inne", "Hjemme"],
      "description": "Where session took place"
    },
    "Værforhold": {
      "type": "select",
      "options": ["Perfekt", "Bra", "Ok", "Utfordrende", "Dårlig"],
      "description": "Weather conditions"
    },
    "Trener_tilstede": {
      "type": "checkbox",
      "description": "Was coach present?"
    },
    "Trener_navn": {
      "type": "people",
      "description": "Coach who supervised/observed"
    },
    "Skader_plager": {
      "type": "text",
      "description": "Any injuries or physical issues noted"
    },
    "Utstyr_brukt": {
      "type": "multi_select",
      "options": [
        "Launch monitor",
        "Video",
        "Alignment sticks",
        "Putting mirror",
        "Gym equipment",
        "Annet"
      ],
      "description": "Equipment used"
    },
    "Tags": {
      "type": "multi_select",
      "options": [
        "Breakthrough",
        "Frustrasjon",
        "Morsom",
        "Utfordrende",
        "Rutinemessig",
        "Kreativ",
        "Experiment"
      ],
      "description": "Session tags/mood"
    }
  },
  "views": [
    {
      "name": "Alle logger",
      "type": "table",
      "sort": [{"property": "Dato", "direction": "descending"}]
    },
    {
      "name": "Denne uken",
      "type": "table",
      "filter": {"property": "Dato", "date": {"past_week": {}}},
      "sort": [{"property": "Dato", "direction": "descending"}]
    },
    {
      "name": "Per spiller",
      "type": "table",
      "group_by": "Spiller",
      "sort": [{"property": "Dato", "direction": "descending"}]
    },
    {
      "name": "Gjennomført økter",
      "type": "table",
      "filter": {"property": "Gjennomført", "checkbox": {"equals": true}}
    },
    {
      "name": "Ikke gjennomført",
      "type": "table",
      "filter": {"property": "Gjennomført", "checkbox": {"equals": false}}
    },
    {
      "name": "Høy kvalitet (4-5)",
      "type": "table",
      "filter": {
        "or": [
          {"property": "Kvalitet", "select": {"equals": "5 - Utmerket"}},
          {"property": "Kvalitet", "select": {"equals": "4 - Bra"}}
        ]
      }
    },
    {
      "name": "Lav kvalitet (1-2)",
      "type": "table",
      "filter": {
        "or": [
          {"property": "Kvalitet", "select": {"equals": "1 - Dårlig"}},
          {"property": "Kvalitet", "select": {"equals": "2 - Under forventning"}}
        ]
      }
    },
    {
      "name": "Timeline",
      "type": "timeline",
      "timeline_by": "Dato",
      "group_by": "Spiller"
    },
    {
      "name": "Completion tracking",
      "type": "table",
      "properties_visible": ["Dato", "Spiller", "Økt_type", "Gjennomført", "Kvalitet", "Varighet_faktisk"]
    }
  ],
  "analytics": {
    "player_metrics": [
      "Completion rate (% sessions completed vs planned)",
      "Average quality rating",
      "Average session duration",
      "Training volume (hours per week)",
      "Breaking point work frequency"
    ],
    "coach_insights": [
      "Identify patterns (low quality on certain days/times)",
      "Track energy levels (detect overtraining)",
      "Monitor missed sessions (early intervention)",
      "Assess training effectiveness (quality trends)"
    ]
  },
  "usage_notes": {
    "player_responsibility": [
      "Log every planned session (completed or not)",
      "Fill in within 24 hours of session",
      "Be honest with quality and focus ratings",
      "Note specific results when possible"
    ],
    "coach_responsibility": [
      "Review logs weekly",
      "Add feedback when quality is very high or very low",
      "Identify patterns requiring intervention",
      "Use data to adjust training plans"
    ]
  }
}
