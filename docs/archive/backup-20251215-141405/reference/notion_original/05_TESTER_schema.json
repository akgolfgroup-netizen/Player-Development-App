{
  "database_name": "TESTER",
  "description": "Individual test results tracking - 20 Team Norway tests",
  "properties": {
    "Testnavn": {
      "type": "title",
      "description": "Test name and number (Test 1-20)"
    },
    "Spiller": {
      "type": "relation",
      "related_database": "SPILLERE",
      "description": "Link to player profile"
    },
    "Dato": {
      "type": "date",
      "description": "Test date"
    },
    "Uke": {
      "type": "number",
      "description": "Week number when test was performed"
    },
    "Testkategori": {
      "type": "select",
      "options": [
        "Golf Shots (1-7)",
        "Teknikk (8-11)",
        "Fysisk (12-14)",
        "Mental (15-18)",
        "Strategisk (19-20)"
      ],
      "description": "Test category grouping"
    },
    "Resultat": {
      "type": "number",
      "format": "decimal",
      "description": "Actual test result"
    },
    "Enhet": {
      "type": "select",
      "options": ["meter", "kg", "mph", "rpm", "grader", "sekunder", "minutter", "%", "poeng"],
      "description": "Unit of measurement"
    },
    "Krav_for_kategori": {
      "type": "number",
      "format": "decimal",
      "description": "Requirement for player's current category"
    },
    "Kategori_testet_som": {
      "type": "select",
      "options": ["A", "B", "C", "D", "E", "F", "G", "H", "I", "J", "K"],
      "description": "Category player was tested as"
    },
    "Differanse": {
      "type": "formula",
      "formula": "prop('Resultat') - prop('Krav_for_kategori')",
      "description": "Difference from requirement (positive = exceeded, negative = below)"
    },
    "Godkjent": {
      "type": "checkbox",
      "description": "Met or exceeded category requirement"
    },
    "Prosent_av_krav": {
      "type": "formula",
      "formula": "(prop('Resultat') / prop('Krav_for_kategori')) * 100",
      "format": "percent",
      "description": "Result as percentage of requirement"
    },
    "Notater": {
      "type": "text",
      "description": "Test notes, conditions, observations"
    },
    "Video_link": {
      "type": "url",
      "description": "Link to video recording of test"
    },
    "Trener_kommentar": {
      "type": "text",
      "description": "Coach comments on result"
    },
    "Forrige_resultat": {
      "type": "number",
      "description": "Previous test result for comparison"
    },
    "Forbedring": {
      "type": "formula",
      "formula": "prop('Resultat') - prop('Forrige_resultat')",
      "description": "Improvement from previous test"
    },
    "Neste_test_dato": {
      "type": "date",
      "description": "Scheduled date for next test"
    }
  },
  "views": [
    {
      "name": "Alle tester",
      "type": "table",
      "sort": [
        {"property": "Dato", "direction": "descending"},
        {"property": "Spiller", "direction": "ascending"}
      ]
    },
    {
      "name": "Per testkategori",
      "type": "board",
      "group_by": "Testkategori"
    },
    {
      "name": "Godkjente tester",
      "type": "table",
      "filter": {"property": "Godkjent", "checkbox": {"equals": true}}
    },
    {
      "name": "Ikke godkjente (bruddpunkter)",
      "type": "table",
      "filter": {"property": "Godkjent", "checkbox": {"equals": false}}
    },
    {
      "name": "Per spiller",
      "type": "table",
      "group_by": "Spiller",
      "sort": [{"property": "Testnavn", "direction": "ascending"}]
    },
    {
      "name": "Siste benchmark",
      "type": "table",
      "filter": {"property": "Dato", "date": {"past_week": {}}},
      "sort": [{"property": "Spiller", "direction": "ascending"}]
    }
  ],
  "test_reference": {
    "Golf_Shots_1_7": [
      "Test 1: Driver avstand (carry, meter)",
      "Test 2: Jern 7 avstand (carry, meter)",
      "Test 3: Jern 7 nøyaktighet (6 slag til target)",
      "Test 4: Wedge PEI (Precision Efficiency Index)",
      "Test 5: Lag-kontroll putting (9 putts, 3-6-9 meter)",
      "Test 6: Lesing putting (6 putts, 3 meter med break)",
      "Test 7: Bunker (10 slag, % innenfor 3m)"
    ],
    "Teknikk_8_11": [
      "Test 8: Klubbfart driver (Clubspeed, mph)",
      "Test 9: Smash factor driver (Ball speed / Club speed)",
      "Test 10: Launch angle driver (grader)",
      "Test 11: Spin rate driver (rpm)"
    ],
    "Fysisk_12_14": [
      "Test 12: Benkpress (1RM, kg)",
      "Test 13: Markløft trapbar (1RM, kg)",
      "Test 14: Rotasjonskast 4kg medisinball (distanse, meter)"
    ],
    "Mental_15_18": [
      "Test 15: Pressure Putting (10 putts @ 2m, eliminering)",
      "Test 16: Pre-shot Rutine Konsistens (videoanalyse 18 hull)",
      "Test 17: Fokus under distraksjon (% treff vs baseline)",
      "Test 18: Mental Toughness Questionnaire (MTQ48)"
    ],
    "Strategisk_19_20": [
      "Test 19: Klubbvalg og Risikovurdering (20 scenarios)",
      "Test 20: Banestrategi-planlegging (18-hulls plan)"
    ]
  }
}
